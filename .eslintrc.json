{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true,
      "jquery": true
    },
    "globals": {
      "chrome": true,
      "gAuthenticationModel": true,
      "gNavManager": true,
      "models": true,
      "services": true,
      "repositories": true,
      "enums": true,
      "fut_guid": true,
      "gUserModel": true
    },
    "extends": [ "eslint:recommended", "plugin:react/recommended" ],
    "parserOptions": {
        "ecmaVersion": 2017,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": [
          "off"
        ],
        "indent": [
            "error",
            2,
            { "flatTernaryExpressions": true, "SwitchCase": 1 }
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "react/prop-types": "off"
    }
}
